<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cube.dao.PackageDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.cube.entity.PackageEntity" id="packageMap">
        <result property="id" column="id"/>
        <result property="buildVersion" column="build_version"/>
        <result property="bundleid" column="bundleid"/>
        <result property="createTime" column="create_time"/>
        <result property="extra" column="extra"/>
        <result property="fileName" column="file_name"/>
        <result property="minVersion" column="min_version"/>
        <result property="name" column="name"/>
        <result property="platform" column="platform"/>
        <result property="size" column="size"/>
        <result property="version" column="version"/>
        <result property="provisionId" column="provision_id"/>
        <result property="status" column="status"/>
        <result property="changeLog" column="change_log"/>
    </resultMap>


</mapper>