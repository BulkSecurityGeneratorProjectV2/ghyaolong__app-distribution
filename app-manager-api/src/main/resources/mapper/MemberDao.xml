<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cube.dao.MemberDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.cube.entity.MemberEntity" id="memberMap">
        <result property="id" column="id"/>
        <result property="userName" column="user_name"/>
        <result property="password" column="password"/>
        <result property="tel" column="tel"/>
        <result property="realName" column="real_name"/>
        <result property="idNumber" column="id_number"/>
        <result property="address" column="address"/>
        <result property="qq" column="qq"/>
        <result property="company" column="company"/>
        <result property="position" column="position"/>
        <result property="avatar" column="avatar"/>
        <result property="createTime" column="create_time"/>
        <result property="status" column="status"/>
        <result property="isForbidden" column="is_forbidden"/>
        <result property="downloadCount" column="download_count"/>
        <result property="frontPhotoPath" column="front_photo_path"/>
        <result property="backPhotoPath" column="back_photo_path"/>
        <result property="handPhotoPath" column="hand_photo_path"/>
        <result property="frontPhotoOssPath" column="front_photo_oss_path"/>
        <result property="backPhotoOssPath" column="back_photo_oss_path"/>
